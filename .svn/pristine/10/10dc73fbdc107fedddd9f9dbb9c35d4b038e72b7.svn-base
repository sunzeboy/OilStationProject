<?xml version="1.0" encoding="utf-8"?>
<cn.sh.changxing.ct.mobile.view.lbs.MapOperViewLayer xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <TextView
        android:id="@+id/txt_map_oper_switch_traffic_enabled_flag"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginEnd="@dimen/btn_fragment_map_oper_marginRight"
        android:layout_marginRight="@dimen/btn_fragment_map_oper_marginRight"
        android:layout_marginTop="@dimen/btn_fragment_map_oper_road_detail_marginTop"
        android:background="@drawable/selector_btn_lbs_map_road_traffic_info_off"
        android:gravity="bottom|center_horizontal"
        android:maxLength="10"
        android:paddingBottom="3dp"
        android:singleLine="true"
        android:text="@string/lbs_road_traffic_info"
        android:textColor="@color/selector_lbs_image_label_btn_text_color"
        android:textSize="@dimen/lbs_map_oper_image_label_btn_textsize" />

    <ImageButton
        android:id="@+id/btn_map_oper_zoom_in"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/txt_map_oper_switch_traffic_enabled_flag"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/txt_map_oper_switch_traffic_enabled_flag"
        android:layout_marginEnd="@dimen/btn_fragment_map_oper_marginRight"
        android:layout_marginTop="28dp"
        android:background="@drawable/selector_btn_lbs_map_zoomin"
        android:contentDescription="@string/app_name" />

    <ImageButton
        android:id="@+id/btn_map_oper_zoom_out"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/txt_map_oper_switch_traffic_enabled_flag"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/btn_map_oper_zoom_in"
        android:layout_marginEnd="@dimen/btn_fragment_map_oper_marginRight"
        android:background="@drawable/selector_btn_lbs_map_zoomout"
        android:contentDescription="@string/app_name" />

    <ImageButton
        android:id="@+id/btn_map_oper_my_location"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/btn_map_oper_zoom_out"
        android:layout_below="@+id/btn_map_oper_zoom_out"
        android:layout_marginTop="97dp"
        android:background="@drawable/selector_btn_lbs_map_north_up"
        android:contentDescription="@string/app_name" />

</cn.sh.changxing.ct.mobile.view.lbs.MapOperViewLayer>