<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="cn.sh.changxing.ct.mobile.activity.LbsActivity" >
    
    <fragment   
        android:name="cn.sh.changxing.ct.mobile.fragment.lbs.MapViewFragment"
        android:id="@+id/map_view_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
    
    <RelativeLayout
        android:id="@+id/map_oper_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@null">
    </RelativeLayout>
    
    <RelativeLayout
        android:id="@+id/lbs_full_cover_ui_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@null" >
    </RelativeLayout>
    
    <RelativeLayout
        android:id="@+id/map_display_set_ui_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@null" >
    </RelativeLayout>

</RelativeLayout>
